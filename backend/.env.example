# Modulo: backend
# Arquivo: .env.example
# Funcao no sistema: modelo de variaveis de ambiente sem segredos reais.

PORT=3001
DB_SSL=require
FRONTEND_ORIGIN=*
DATABASE_URL=postgresql://<user>:<password>@<host>:5432/<database>

# Modulo: backend/auth
# Observacao: quando AUTH_ENABLED=true, a UI deve exigir login e a API passa a exigir Bearer token.
AUTH_ENABLED=false
AUTH_JWT_SECRET=<defina_um_segredo_forte_aqui>
AUTH_JWT_EXPIRES_IN=12h
